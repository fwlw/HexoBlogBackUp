#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="$basedir/D:/Program/blog/node_modules/.store/swig@1.4.1/node_modules"
else
  export NODE_PATH="$NODE_PATH:$basedir/D:/Program/blog/node_modules/.store/swig@1.4.1/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../swig@1.4.1/node_modules/swig/bin/swig.js" "$@"
else
  exec node  "$basedir/../../../../../swig@1.4.1/node_modules/swig/bin/swig.js" "$@"
fi
